{
    "schemaVersion": 1,
    "id": "persistentstorage",
    "version": "${version}",
    "name": "Persistent Storage",
    "description": "Allows mods to access storage between worlds",
    "authors": [
        "Sygikal"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "CC0-1.0",
    "icon": "assets/persistentstorage/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
          "dev.sygii.persistentstorage.PersistentStorage"
        ]
    },
    "mixins": [
        "persistentstorage.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.16.14",
        "minecraft": "${mcdep}",
        "java": "${javadep}",
        "fabric-api": "*"
